{
  "name": "next-routify",
  "version": "1.5.2",
  "main": "dist/index.js",
  "repository": "https://github.com/AlfonzAlfonz/next-routify.git",
  "author": "Denis HomoliÃÅk <denis@homolik.cz>",
  "description": "Routing alternative for safe urls with typed parameters",
  "license": "MIT",
  "scripts": {
    "lint": "yarn tslint -c tslint.json 'src/**/*.ts' --project tsconfig.json",
    "test": "mocha --require ts-node/register test/**/*.ts",
    "coverage": "nyc mocha test/**/*.ts",
    "build": "yarn tsc",
    "debug": "tsc && yarn mocha --debug-brk",
    "ci": "yarn lint && yarn build && yarn test",
    "prepublishOnly": "npm run ci"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/expect": "^1.20.4",
    "@types/mocha": "^5.2.6",
    "@types/next-server": "^8.1.1",
    "@types/react": "^16.8.6",
    "chai": "^4.2.0",
    "mocha": "^6.0.2",
    "next": "^8.1.0",
    "nyc": "^14.1.1",
    "prettier": "^1.16.4",
    "react": "^16.8.6",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.1"
  },
  "peerDependencies": {
    "next": "^8.0.3",
    "next-server": "^8.0.3",
    "react": "^16.8.3"
  },
  "dependencies": {
    "@types/next": "^8.0.5",
    "path-to-regexp": "^3.0.0"
  },
  "keywords": [
    "next",
    "next.js",
    "react",
    "route",
    "router",
    "routes",
    "routing",
    "routify"
  ],
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  }
}